{
  "multipart": [
    {
      "when": {
        "west": "low","east":"low","north":"low","south":"low"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_post"
      }
    },
    {
      "when": {
        "west": "tall","east":"tall","north":"tall","south":"tall"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_post"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_post"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "low"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "low"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "low"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_side_tall",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_side_tall",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_side_tall",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "xkdeco:block/steel_column_head_side_tall",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}