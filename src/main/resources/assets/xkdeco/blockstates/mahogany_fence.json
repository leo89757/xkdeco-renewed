{
  "multipart": [
    {
      "apply": {
        "model": "xkdeco:block/mahogany_fence_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "xkdeco:block/mahogany_fence"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "xkdeco:block/mahogany_fence",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "xkdeco:block/mahogany_fence",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "xkdeco:block/mahogany_fence",
        "y": 90
      }
    }
  ]
}